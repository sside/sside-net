{
    "name": "@sside-net/project-logger",
    "version": "0.0.0",
    "description": "sside.net shared logger",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsc --watch",
        "dev:shared": "npm run dev",
        "build": "rimraf dist && tsc --build",
        "build:shared": "npm run build",
        "lint": "eslint --fix",
        "test": "dotenvx run --env-file ../../.env.test -- jest",
        "update:check": "ncu",
        "update:execute": "ncu -u"
    },
    "dependencies": {
        "pino": "^10.0.0",
        "utility-types": "^3.11.0"
    },
    "devDependencies": {
        "@sside-net/eslint": "*",
        "@sside-net/jest": "*",
        "@sside-net/tsconfig": "*",
        "rimraf": "^6.0.1",
        "typescript": "^5.9.3"
    },
    "private": true
}
