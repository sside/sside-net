    },
    "devDependencies": {
        "@sside-net/eslint": "*",
        "@sside-net/tsconfig": "*",
        "@sside-net/jest": "*",
        "rimraf": "^6.0.1",
        "typescript": "^5.9.2"
    },
    "private": true
}
