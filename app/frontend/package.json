{
    "name": "@sside-net/frontend",
    "version": "0.0.0",
    "private": true,
    "description": "sside.net frontend",
    "scripts": {
        "start": "next start",
        "dev": "dotenvx run --env-file ../../.env -- cross-env PORT=42979 next dev --turbopack",
        "build": "next build",
        "test": "cross-env PORT=42979 playwright test",
        "test:ui": "cross-env PORT=42979 playwright test --ui",
        "lint": "eslint --fix",
        "update:check": "ncu",
        "update:execute": "ncu -u"
    },
    "dependencies": {
        "cross-env": "10.1.0",
        "next": "16.0.3",
        "react": "19.2.0",
        "react-dom": "19.2.0"
    },
    "devDependencies": {
        "@dotenvx/dotenvx": "1.51.1",
        "@next/eslint-plugin-next": "^15.5.6",
        "@playwright/test": "1.56.1",
        "@sside-net/eslint": "*",
        "@tailwindcss/postcss": "4.1.17",
        "@types/node": "^22",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.17",
        "typescript": "^5.9.3"
    }
}
